<body class="bg-gray js-body">
<div class="navbar nav-bg bottom-border">
    <div class="l-padding-horizontal">
        <div class="navbar-collapse">
            <ul class="nav navbar-nav shadow full-width">
                <li class="transition">
                    <a href="/manager" class="color-white transition <%if(locals.currentMenu&&currentMenu=="home"){%>current-menu<%}%>" title="误码者后台管理">
                        <i class="glyphicon glyphicon-globe"></i>&nbsp;误码者后台管理
                    </a>
                </li>
                <% if(session.user != null){ %>
                <li>
                    <a href="/manager/content?manager=true&page=1" class="color-white js-settings-btn transition <%if(locals.currentMenu&&currentMenu=="content"){%>current-menu<%}%>">
                        <i class="glyphicon glyphicon-book"></i>&nbsp;日志管理
                    </a>
                </li>
                <li>
                    <a href="/manager/user?page=1" class="color-white js-settings-btn transition <%if(locals.currentMenu&&currentMenu=="user"){%>current-menu<%}%>">
                        <i class="glyphicon glyphicon-user"></i>&nbsp;用户管理
                    </a>
                </li>
                <% if(session.user.role == 0){ %>
                <li>
                    <a href="/manager/category?page=1" class="color-white js-settings-btn transition <%if(locals.currentMenu&&currentMenu=="category"){%>current-menu<%}%>">
                        <i class="glyphicon glyphicon-list"></i>&nbsp;分类管理
                    </a>
                </li>
                <li>
                    <a href="/manager/log?view=manager/log&page=1" class="color-white js-settings-btn transition <%if(locals.currentMenu&&currentMenu=="log"){%>current-menu<%}%>">
                        <i class="glyphicon glyphicon-volume-up"></i>&nbsp;公告管理
                    </a>
                </li>
                <%}%>
                <li>
                    <a href="/" class="color-white js-settings-btn transition">
                        <i class="glyphicon glyphicon-home"></i>&nbsp;前台首页
                    </a>
                </li>
                <li><a class="color-white js-user-center transition">
                        <%= session.user.role == 0 ? '管理员' : 'VIP用户' %>
                        :&nbsp;<%= session.user.realName ? session.user.realName : session.user.userName %></a>
                </li>
                <li><a href="javascript:void(0)" class="color-white js-logout-btn transition" title="退出系统">
                        <i class="glyphicon glyphicon-log-out"></i>&nbsp;退出
                    </a>
                </li>
                <% }else{ %>
                <% } %>
            </ul>
        </div>
    </div>
</div>
